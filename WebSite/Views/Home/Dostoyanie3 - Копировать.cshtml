        //    $('.arrow').addClass(thisClass + "-arrow");
        //});

        $('[data-toggle="tooltip"]').hover(function () {
            $('.tooltip-inner').css('min-width', '400px');
            $('.tooltip-inner').css('color', 'white');
            $('.tooltip-inner').css('background-color', '#1c7de0');

        });
    </script>
}
